1.
STORE INPUT WITH VALUE "MASSA BENDA 600 kg"
STORE INPUT WITH VALUE "PERCEPATAN BENDA 2 m/s2"
CALCULATE "MASSA BENDA 600 kg" TIMES "PERCEPATAN BENDA 2 m/s2"
SET "GAYA" value with calculation result
DISPLAY "GAYA"

2.
STORE INPUT WITH ANY VALUE "TAHUN"
IF "TAHUN" MODUL0 4 IS 0
  IF "TAHUN" MODUL0 100 IS NOT 0
      DISPLAY "TAHUN KABISAT"
  ELSE IF "TAHUN" MODULO 100 IS 0 AND "TAHUN" MODULO 400 IS 0
      DISPLAY "TAHUN KABISAT"
  ELSE 
      DISPLAY "BUKAN TAHUN KABISAT"
ELSE 
      DISPLAY "BUKAN TAHUN KABISAT"

3.
STORE INPUT WITH VALUE "PAKAIAN YANG DICUCI = 20"
STORE INPUT WITH VALUE "PAKAIAN MASUK MESIN CUCI =0"
WHILE "PAKAIAN MASUK MESIN CUCI < PAKAIAN YANG DICUCI"
  DO  IF "PAKAIAN MASUK MESIN CUCI" = TRUE
	"PAKAIAN MASUK MESIN CUCI" = "PAKAIAN MASUK MESIN CUCI" + 1
DISPLAY RESULT "PAKAIAN MASUK MESIN CUCI"

4.
STORE INPUT WITH VALUE "TOTAL SISWA/SISWI" = 40"
STORE INPUT WITH VALUE "SISWA/SISWI YANG DIPERIKSA" = 0"
WHILE "SISWA/SISWI YANG DIPERIKSA" < "TOTAL SISWA/SISWI"
  DO  IF "KUKU SISWA/SISWI PANJANG" = TRUE
	  DISPLAY "GURU AKAN MENGHUKUM SISWA/SISWI TERSEBUT"
      ELSE        
          DISPLAY "GURU AKAN MEMUJI SISWA/SISWI TERSEBUT"




		



